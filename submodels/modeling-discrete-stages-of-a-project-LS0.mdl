"Stages to Go!"= INTEG (
	-Internal ask Rate,
		(Last pulse time + Repeat Interval)/Repeat Interval)
	~	stages
	~		|

Current Stage=
	1 + Stages Completed
	~	stages
	~		|

Current Technology Readiness Level= WITH LOOKUP (
	Current Stage,
		([(0,0)-(11,100)],(1,0),(2,10),(3,20),(4,30),(5,40),(6,50),(7,60),(8,70),(9,80),(10\
		,90),(11,100) ))
	~	
	~		|

Duration=
	1
	~	Month
	~		|

Expected Technology Readiness Level= WITH LOOKUP (
	Current Stage,
		([(0,0)-(11,50)],(1,0),(2,5),(3,10),(4,15),(5,20),(6,25),(7,30),(8,35),(9,40),(10,45\
		),(11,50) ))
	~	
	~		|

First pulse time=
	0
	~	Month
	~		|

Internal ask Rate=
	PULSE TRAIN(First pulse time, Duration , Repeat Interval , Last pulse time )*"Project \"On Time\" Binary"
	~	stages/Month
	~		|

Last pulse time=
	54
	~	Month
	~		|

"Project \"On Time\" Binary"=
	IF THEN ELSE(Current Technology Readiness Level >= Expected Technology Readiness Level\
		, 1 , 0 )
	~	
	~		|

Repeat Interval=
	6
	~	Month
	~		|

Stages Completed= INTEG (
	Internal ask Rate,
		0)
	~	stages
	~		|

