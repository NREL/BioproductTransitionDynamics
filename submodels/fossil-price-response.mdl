{UTF-8}
fossil price response=
	fossil price response magnitude * Current Fossil Price
	~	USD/tonFeedstock [0,?]
	~		|

changing fossil price=
	fossil monthly change in price
	~	USD/tonFeedstock/year
	~		|

fossil maximum fluctuation=
	0.05
	~	Dmnl
	~		|

Current Fossil Price= INTEG (
	changing fossil price,
		fossil starting price)
	~	USD/tonFeedstock [0,?]
	~		|

fossil starting price=
	1600
	~	USD/tonFeedstock
	~		|

fossil price response magnitude=
	0.2
	~	Dmnl [0,1,0.05]
	~		|

fossil price trend=
	fossil starting price +
	( fossil long term price change*fossil starting price / (FINAL TIME - INITIAL TIME) \
		) * (Time - INITIAL TIME)
	- fossil price response
	~	USD/tonFeedstock
	~		|

fossil reversion time=
	0.1
	~	year
	~		|

fossil monthly change in price=
	( fossil price trend - Current Fossil Price ) / fossil reversion time
	+ fossil price trend
	* RANDOM NORMAL(-fossil maximum fluctuation,
	                fossil maximum fluctuation,
	                0,
	                fossil maximum fluctuation,
	                random stream)/TIME STEP
	~	USD/tonFeedstock/year
	~		|

fossil long term price change=
	0.0625
	~	Dmnl
	~		|

random stream=
	0
	~	Dmnl [0,?,1]
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 2050
	~	year
	~	The final time for the simulation.
	|

INITIAL TIME  = 2015
	~	year
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	year [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 0.03125
	~	year [0,?]
	~	The time step for the simulation.
	|

