{UTF-8}
feedstock array:
	corn, switchgrass, perennial
	~	
	~		|

price=
	select array check * ( price array[corn] * select array[corn]
	  + price array[switchgrass] * select array[switchgrass]
	  + price array[perennial] * select array[perennial] )
	~	
	~		|

price A= WITH LOOKUP (
	Time,
		([(0,0)-(100,60)],(0,10),(10,20),(30,30),(50,50),(60,50),(100,55) ))
	~	
	~		|

price array[corn]=
	price A ~~|
price array[switchgrass]=
	price B ~~|
price array[perennial]=
	price C
	~	
	~		|

price B= WITH LOOKUP (
	Time,
		([(0,0)-(100,20)],(0,20),(100,20) ))
	~	
	~		|

price C= WITH LOOKUP (
	Time,
		([(0,0)-(100,60)],(0,55),(50,25),(100,10) ))
	~	
	~		|

select array[corn]=
	1 ~~|
select array[switchgrass]=
	0 ~~|
select array[perennial]=
	0
	~	 [0,1,1]
	~		|

select array check=
	IF THEN ELSE(SUM(select array[feedstock array!]) = 1, 1, 0)
	~	
	~		|

