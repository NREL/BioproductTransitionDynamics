	SpendingDamper(Commercial Capital Expense, commercial capital cost, commercial construction time\
		, TIME STEP)
	~	USD/year [0,?]
	~		|

piloting damper=
	HoursDamper(Continuous Piloting Hours, actual piloting rate, TIME STEP, one Year)
	~	hours/year
	~		|

demoing damper=
	HoursDamper(Continuous Demoing Hours, actual demoing rate, TIME STEP, one Year)
	~	hours/year
	~		|

demoing capital expenditure damper=
	SpendingDamper(Demo Capital Expense, demo capital cost, demo construction time, TIME STEP\
		)
	~	USD/year [0,?]
	~		|

startup demoing damper=
	HoursDamper(Startup Demoing Hours, startup demoing rate, TIME STEP, one Year)
	~	hours/year [0,8766]
	~		|

startup piloting damper=
	HoursDamper(Startup Piloting Hours, startup piloting rate, TIME STEP, one Year)
	~	hours/year [0,8766]
	~		|

regulatory process spending damper=
	SpendingDamper(Regulatory Costs, total approval cost, total approval time, TIME STEP\
		)
	~	USD/year [0,?]
	~		|

piloting capital expenditure damper=
	SpendingDamper(Pilot Capital Expense, pilot capital cost, pilot construction time, TIME STEP\
		)
	~	USD/year [0,?]
	~		|

********************************************************
	.stages
********************************************************~
	|

regulatory process ongoing=
	IF THEN ELSE(Completed Demoing >= regulatory process starting point * target demo hours
	             :AND: Regulatory Costs > 0,
	             1,
	             0)
	~	Dmnl [-1,1]
	~	The regulatory process begins once sufficient demoing hours - not \
		necessarily continuous - are completed.
	|

commercial plant construction=
	demoing complete * (1 - regulatory delay) * (1 - precommercial) *
	IF THEN ELSE(Working Capital >= TIME STEP * commercial capital cost / commercial construction time
	             :AND: Commercial Capital Expense > 0,
	             1,
	             0)
	~	Dmnl [0,?]
	~		|

commercial plant is built=
	IF THEN ELSE(Commercial Capital Expense <= 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

commercial plant operation=
	IF THEN ELSE(commercial plant is built = 1
	             :AND: profitability indicator = 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

demo plant construction=
	piloting complete * (1 - predemoing) *
	IF THEN ELSE(Working Capital >= TIME STEP * demo capital cost / demo construction time
	             :AND: Demo Capital Expense > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

demo plant is built=
	IF THEN ELSE(Demo Capital Expense <= 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

demoing complete=
	IF THEN ELSE(Continuous Demoing Hours <= 0, 
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

demoing ongoing=
	IF THEN ELSE(demo plant is built = 1 :AND: Continuous Demoing Hours > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

precommercial=
	IF THEN ELSE(demoing complete = 1
	             :AND: regulatory delay = 0
	             :AND: Working Capital < Commercial Capital Expense
	             :AND: Commercial Capital Expense = commercial capital cost,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

pilot plant construction=
	(1 - prepiloting) *
	IF THEN ELSE(Working Capital >= TIME STEP * pilot capital cost / pilot construction time
	             :AND: Pilot Capital Expense > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~	Pilot plant construction continues provided there are sufficient funds in \
		Working Capital to satisfy the next time step's worth of construction \
		expenses, and there are remaining construction expenses in Pilot Capital \
		Expense.
	|

pilot plant is built=
	IF THEN ELSE(Pilot Capital Expense <= 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

piloting complete=
	IF THEN ELSE(Continuous Piloting Hours <= 0, 1, 0)
	~	Dmnl [0,1]
	~		|

piloting ongoing=
	IF THEN ELSE(pilot plant is built = 1 :AND: Continuous Piloting Hours > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

prepiloting=
	IF THEN ELSE(Working Capital < pilot capital cost
	             :AND: Pilot Capital Expense = pilot capital cost,
	             1,
	             0)
	~	Dmnl [0,1]
	~	Pre-piloting is the period of gathering funds before pilot plant \
		construction can begin. Once sufficient funds have accumulated in Working \
		Capital to build the plant, construction begins.
	|

predemoing=
	piloting complete *
	IF THEN ELSE(Working Capital < demo capital cost
	             :AND: Demo Capital Expense = demo capital cost,
	             1,
	             0)
	~	Dmnl [0,2]
	~		|

regulatory delay=
	IF THEN ELSE(demoing complete = 1 :AND: Regulatory Costs > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

startup demoing completed=
	IF THEN ELSE(Startup Demoing Hours <= 0,
	             1,
	             0)
	~	Dmnl [0,?]
	~		|

startup piloting complete=
	IF THEN ELSE(Startup Piloting Hours <= 0,
	             1,
	             0)
	~	Dmnl [0,?]
	~		|

********************************************************
	.indicators
********************************************************~
	|

IS equipment depreciation indicator=
	IF THEN ELSE(
	IS Equipment Useful Value > one dollar,
	1,
	0
	)
	~	Dmnl [0,1,1]
	~		|

bioproduct supply to demand ratio indicator=
	IF THEN ELSE(Adopters > 0,
	             production / ( Adopters * bioproduct performance advantage ),
	             1)
	~	Dmnl [0,?]
	~	This indicator returns 1 when Adopters = 0 (indicating no demand for \
		bioproduct). As a result, when there is no demand for bioproduct the \
		bioproduct price model is a pure random walk unaffected by the level of \
		production.
	|

internal investor decision=
	IF THEN ELSE(value threshold >= expected idealized internal net present value
	             :AND: completing stages = 1
	             :AND: base external investor ask rate = 0,
	             1,
	             0)
	~	Dmnl [0,1,1]
	~	Can only be non-zero if the project is internally funded, at the end of an \
		internal project stagegate (when completing stages is non-zero). During \
		those time steps, this parameter is 1 if the value threshold exceeds or \
		equals the project's NPV at that time, and 0 otherwise.
	|

green premium adjusted=
	IF THEN ELSE(Current Incumbent Price / bioproduct performance advantage 
	               - (1 + expected green premium) * Current Bioproduct Price 
	               >= 0,
	             expected green premium,
	             MAX(0,
	                 (Current Incumbent Price / bioproduct performance advantage - Current Bioproduct Price\
		) 
	                   / Current Bioproduct Price)
	             )
	~	Dmnl [0,1]
	~	Checks whether the bioproduct has sufficient cost advantage that the \
		expected green premium can be implemented. If not, the green premium is \
		set to either zero (if the bioproduct price is greater than the incumbent \
		price) or the difference between the incumbent and bioproduct prices.
	|

bioproduct selling price with green premium=
	Current Bioproduct Price * (1 + green premium adjusted)
	~	USD/tonProduct
	~		|

abandoning bioproduct indicator=
	IF THEN ELSE(retrofit delay = 0 
	             :AND: incumbent favorability indicator = 1
	             :AND: Adopters * bioproduct performance advantage > 
	                      bioproduct offtake agreement * commercial plant capacity,
	             1,
	             0)
	~	Dmnl [0,1]
	~	Allows biproduct consumers to switch back to incumbent product only if no \
		retrofitting was done (retrofit delay = 0) and if the incumbent is \
		financially favorable. Only consumers not under an offtake agreement can \
		abandon, hence the greater-than requirement on Adopters.
	|

bioproduct favorability indicator=
	IF THEN ELSE(Current Bioproduct Price <= Current Incumbent Price / bioproduct performance advantage\
		,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

incumbent favorability indicator=
	1 - bioproduct favorability indicator
	~	Dmnl [0,1]
	~		|

internal project on time indicator=
	IF THEN ELSE(technology readiness level >= required technology readiness level,
	             0,
	             1)
	~	Dmnl [0,1]
	~	Returns 1 when the project is behind and 0 when the project is on time \
		according to technology readiness level. This parameter is used to count \
		up missed stagegates
	|

researching indicator=
	IF THEN ELSE(researching > 0,
	             1,
	             0)
	~	Dmnl [0,1]
	~	When the researching flow is non-zero, this indicator reduces the recovery \
		time for the pilot and demo facility.
	|

shared learning from feedstock logistics=
	IF THEN ELSE(Cumulative Feedstock Consumption >= shared learning threshold for feedstock logistics\
		,
	             Cumulative Feedstock Consumption,
	             0)
	~	tonFeedstock [0,?]
	~		|

shared learning from bioproduct production=
	IF THEN ELSE(Cumulative Production > shared learning threshold for bioproduct production\
		,
	             Cumulative Production,
	             0)
	~	tonProduct [0,?]
	~		|

investor ask=
	IF THEN ELSE( ( Time - INTEGER( Time ) ) / TIME STEP < investor ask rate * one Year,
	              1,
	              0)
	~	Dmnl [0,1]
	~	this equation is non-zero when an ask is made and zero otherwise. All asks \
		are clustered at the beginning of the year. Ideal functionality would have \
		asks scattered evenly throughout the year, but a method could not be found \
		to implement the even scattering with the discrete time steps, especially \
		given that the time step might change in the future.
	|

external investor decision=
	IF THEN ELSE( investor ask = 1,
	              RANDOM BINOMIAL( 0 , 1 ,  
	                               probability of successful investor ask ,
	                               1 , 
	                               0 , 1 , random stream ) ,
	              0 )
	~	Dmnl [0,1]
	~	p * time step * ask rate --- gets above 1?
		1 - (1 - p)^{rate*del_t} --- combined with random uniform
	|

actual demoing rate=
	IF THEN ELSE(Demoing Rate < 0,
	             0,
	             IF THEN ELSE(Demoing Rate >= hours per year,
	             hours per year,
	             IF THEN ELSE(Demoing Rate >= demoing acceptable rate,
	                          Demoing Rate,
	                          0)))
	~	hours/year [0,8772]
	~		|

actual piloting rate=
	IF THEN ELSE(Piloting Rate < 0,
	             0,
	             IF THEN ELSE(Piloting Rate >= hours per year,
	             hours per year,
	             IF THEN ELSE(Piloting Rate >= piloting acceptable rate,
	                          Piloting Rate,
	                          0)))
	~	hours/year [0,?]
	~		|

perennial monthly change in price indicator=
	IF THEN ELSE(Time - INTEGER(Time) >= 0.25 :AND: Time - INTEGER(Time) <= 0.5, 1, 0)
	~	Dmnl [0,1]
	~		|

feedstock selection check=
	IF THEN ELSE(SUM( feedstock selector array[feedstock!] ) = 1,
	             1 , 
	             0 )
	~	Dmnl [1,1]
	~		|

repiloting indicator=
	IF THEN ELSE(Continuous Piloting Hours > 0 :AND:
	             piloting < piloting acceptable rate,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

piloting spending indicator=
	IF THEN ELSE( piloting + startup piloting > 0 ,
	              1 ,
	              0 )
	~	Dmnl [0,1]
	~		|

redemoing indicator=
	IF THEN ELSE(Continuous Demoing Hours > 0 :AND:
	             demoing < demoing acceptable rate,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

demoing spending indicator=
	IF THEN ELSE(demoing + startup demoing > 0 ,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

FM positive cash flow=
	IF THEN ELSE( CFS net change of cash and cash equivalents > 0 , Time , 9999 )
	~	year
	~		|

government operating grant amount indicator=
	IF THEN ELSE(Time = INTEGER(Time), 1 / TIME STEP, 0)
	~	1/year
	~		|

actual net income indicator=
	IF THEN ELSE (Commercial Capital Expense > 0, 0, 1)
	~	Dmnl [0,1]
	~		|

research runway response=
	IF THEN ELSE(actual spending >= conserve spending , 
	             0 , 
	             1 - actual spending / conserve spending )
	~	Dmnl [0,?]
	~	Quantifies how the research process is adjusted (how the researching rate \
		is scaled back) when actual spending is above allowable spending but below \
		conserve spending - funds are running low but there are still a few months \
		left. This takes effect following the research response time.
	|

research spending control indicator=
	IF THEN ELSE(actual spending >= conserve spending,
	             0,
	             IF THEN ELSE(actual spending > allowable spending,
	                          research runway response ,
	                          1 ))
	~	Dmnl [0,1]
	~	Implements the research runway response when funds are running low; \
		otherwise has no effect on research team spending. The research runway \
		response is located on the Indicator Variables view.
	|

management spending control indicator=
	IF THEN ELSE(actual spending >= conserve spending,
	             0.5,
	             IF THEN ELSE(actual spending > allowable spending,
	                          management runway response ,
	                          1 )
	             )
	~	Dmnl [0,1]
	~	Implements the management runway response when funds are running low; \
		otherwise has no effect on spending.
	|

idealized pilot operating expenses indicator[forecast year]=
	IF THEN ELSE(
	years ahead[forecast year] >= idealized pilot construction time
	:AND:
	years ahead[forecast year] < idealized pilot completion time,
	1,
	0
	)
	~	Dmnl [0,?]
	~		|

idealized demo operating expenses indicator[forecast year]=
	IF THEN ELSE(years ahead[forecast year] >= idealized demo construction time
	             :AND: years ahead[forecast year] < idealized demo completion time,
	             1,
	             0)
	~	Dmnl
	~		|

idealized commercial operating expenses indicator[forecast year]=
	IF THEN ELSE(years ahead[forecast year] >= idealized commercial construction time,
	             1,
	             0)
	~	Dmnl
	~		|

idealized demo capital expenses indicator[forecast year]=
	IF THEN ELSE(years ahead[forecast year] >= idealized pilot completion time
	             :AND: years ahead[forecast year] < idealized pilot completion time + demo construction time\
		 + one Year / 2,
	             1, 
	             0)
	~	Dmnl
	~		|

idealized commercial capital expenses indicator[forecast year]=
	IF THEN ELSE(years ahead[forecast year] >= idealized demo completion time
	             :AND: years ahead[forecast year] < idealized demo completion time + commercial construction time\
		 + one Year / 2,
	             1,
	             0)
	~	Dmnl
	~		|

idealized demo revenue indicator[forecast year]=
	IF THEN ELSE(years ahead[forecast year] >= idealized demo construction time :AND: years ahead\
		[forecast year] < idealized demo completion time
	,
	             1, 0)
	~	Dmnl
	~		|

pilot and demo spending control indicator=
	IF THEN ELSE(actual spending >= allowable spending :OR: actual spending >= conserve spending\
		 ,
	             pilot and demo runway response,
	             1 )
	~	Dmnl [0,1]
	~	Implements the pilot and demo runway response when funds are running low; \
		otherwise has no effect on spending.
	|

in business indicator=
	IF THEN ELSE( BS equity >= BS equity threshold,
	              1,
	              0 )
	~	Dmnl [0,1]
	~		|

profitability indicator=
	IF THEN ELSE(hypothetical net margin > 0, 1, 0)
	~	Dmnl [0,1]
	~		|

idealized commercial revenue indicator[forecast year]=
	IF THEN ELSE (years ahead[forecast year] >= idealized commercial construction time, \
		1, 0)
	~	Dmnl
	~		|

lower bound on probability=
	IF THEN ELSE( NPV at required return + strategic value to external investors > 0:AND:
	              required payback period - payback period > 0,
	              0.25 ,
	              0 )
	~	Dmnl [0,1]
	~		|

upper bound on probability=
	IF THEN ELSE( NPV at required return + strategic value to external investors > 0:AND:
	              required payback period - payback period > 0,
	              0.75 ,
	              0.5 )
	~	Dmnl [0,1]
	~		|

granting decision=
	IF THEN ELSE(NPV at required return + strategic value to external investors >= 0 
	              :AND: payback period <= required payback period,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

********************************************************
	.timeline
********************************************************~
	|

idealized commercial construction time=
	idealized demo completion time
	+ commercial construction time
	* (1 - commercial plant is built)
	~	year [0,?]
	~		|

idealized demo construction time=
	idealized pilot completion time
	+
	(1 - demo plant is built) * demo construction time
	~	year [0,?]
	~		|

idealized pilot construction time=
	(1 - pilot plant is built) * pilot construction time
	~	year [0,?]
	~		|

idealized demo completion time=
	idealized demo construction time
	+
	MAX(0, remaining demoing duration)
	~	year [0,?]
	~		|

idealized pilot completion time=
	idealized pilot construction time
	+ MAX(0, remaining piloting duration)
	~	year [0,?]
	~		|

********************************************************
	.idealized forecast
********************************************************~
	|

idealized internal expenses present value=
	SUM(
	    (
	       idealized capital expenses[forecast year!]
	     + idealized operating expenses[forecast year!]
	     + idealized research expenses[forecast year!]
	    )
	    / (1 + one Year * required internal return)^(years ahead[forecast year!] / one Year\
		)
	   )
	   * one Year
	   + idealized regulatory cost present value
	~	USD
	~		~	:SUPPLEMENTARY 
	|

expected idealized internal net present value=
	SUM(
	  (
	    idealized revenue[forecast year!] * probability of internal project success
	  - idealized capital expenses[forecast year!]
	  - idealized operating expenses[forecast year!]
	  - idealized research expenses[forecast year!]
	  )
	  / (1 + one Year * required internal return)^(years ahead[forecast year!] / one Year\
		)
	)
	* one Year
	- idealized regulatory cost present value
	+ value of intellectual property * probability of internal project success
	~	USD
	~		|

idealized regulatory wait=
	total approval time
	* Regulatory Costs
	/ total approval cost
	~	year [0,?]
	~		|

idealized research expenses[forecast year]=
	researching
	~	USD/year
	~		|

idealized demo revenue[forecast year]=
	idealized demo revenue indicator[forecast year] *
	include demo in cumulative production * 
	demo capacity * 
	(bioproduct selling price with green premium + government production incentive * expected continuity of government policy\
		)
	~	USD/year
	~		|

idealized commercial revenue[forecast year]=
	idealized commercial revenue indicator[forecast year] * 
	commercial plant capacity * 
	(bioproduct selling price with green premium + government production incentive * expected continuity of government policy\
		)
	~	USD/year
	~		|

idealized NPV=
	SUM(
	  (
	    idealized revenue[forecast year!]
	  - idealized capital expenses[forecast year!]
	  - idealized operating expenses[forecast year!]
	  - idealized research expenses[forecast year!]
	  )
	  / (1 + one Year * required return)^(years ahead[forecast year!] / one Year)
	)
	* one Year
	- idealized regulatory cost present value
	+ value of intellectual property
	~	USD
	~		|

idealized regulatory cost present value=
	Regulatory Costs
	* IF THEN ELSE(
	               idealized regulatory wait > 0,
	               (1 - EXP(- required return * idealized regulatory wait))
	                   / required return / idealized regulatory wait,
	               1
	              )
	~	USD
	~		|

idealized pilot capital expenses[forecast year]=
	IF THEN ELSE(
	    Time + years ahead[forecast year] <= INITIAL TIME + pilot construction time + one Year\
		 / 2,
	    Pilot Capital Expense / INTEGER(pilot construction time  + one Year / 2),
	    0
	)
	~	USD/year [0,?]
	~		|

idealized operating expenses[forecast year]=
	researching * (1 - government research cost share * expected continuity of government policy\
		)
	+ idealized pilot operating expenses[forecast year]
	+ idealized demo operating expenses[forecast year]
	+ idealized commercial operating expenses[forecast year]
	~	USD/year [0,?]
	~		|

idealized pilot operating expenses[forecast year]=
	idealized pilot operating expenses indicator[forecast year] * (pilot opex + piloting management overhead\
		)
	~	USD/year
	~		|

idealized demo operating expenses[forecast year]=
	idealized demo operating expenses indicator[forecast year] * (demo opex + demoing management overhead\
		)
	~	USD/year
	~		|

idealized commercial operating expenses[forecast year]=
	idealized commercial operating expenses indicator[forecast year] * 
	(commercial fixed operating cost + commercial variable operating cost + commercial feedstock cost\
		)
	~	USD/year
	~		|

idealized demo capital expenses[forecast year]=
	idealized demo capital expenses indicator[forecast year] * 
	Demo Capital Expense / INTEGER(demo construction time + one Year / 2)
	~	USD/year
	~		|

idealized capital expenses[forecast year]=
	(
	  idealized pilot capital expenses[forecast year]
	+ idealized demo capital expenses[forecast year]
	+ idealized commercial capital expenses[forecast year]
	)
	* (1 - government capital cost share * expected continuity of government policy)
	~	USD/year
	~		|

idealized commercial capital expenses[forecast year]=
	idealized commercial capital expenses indicator[forecast year] 
	* Commercial Capital Expense / INTEGER(commercial construction time + one Year / 2)
	~	USD/year
	~		|

idealized revenue[forecast year]=
	idealized commercial revenue[forecast year]
	+
	idealized demo revenue[forecast year]
	~	USD/year
	~		|

